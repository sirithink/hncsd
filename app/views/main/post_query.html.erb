<div>
  <p><%= @title %></p>
</div>
<table class="table table-striped table-bordered table-condensed">
  <tr>
    <% @data[0].each do |th| %>
      <th><%=th %></th>
    <% end %>
  </tr>

  <% @data[1..-1].each do |tds| %>
    <tr>
      <% tds.each do |td| %>
        <td><%=td %></td>
      <% end %>
    </tr>
  <% end %>
</table>
